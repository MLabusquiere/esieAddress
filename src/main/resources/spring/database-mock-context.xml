<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean name="contact1" class="fr.esiea.esieaddress.model.Contact">
        <property name="email" value="test@gmail.com"/>
        <property name="dateOfBirth" value="2010-01-31"/>
        <property name="firstName" value="test"/>
        <property name="lastName" value="testLastName"/>
        <property name="actif" value="true"/>
        <property name="id" value="id"/>
    </bean>
    <bean name="database" class="fr.esiea.esieaddress.dao.implementation.DaoContact">
        <property name="database">
            <map>
                <entry key="id" value-ref="contact1" />
            </map>
        </property>
    </bean>
</beans>